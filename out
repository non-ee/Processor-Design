S_0x558659948df0 .scope module, "IF_tb" "IF_tb" 2 3;
 .timescale 0 0;
v0x55865995cc70_0 .var "PC_IN", 31 0;
v0x55865995cd50_0 .net "PC_OUT", 31 0, v0x55865995c850_0;  1 drivers
v0x55865995ce20_0 .var "PC_SRC", 0 0;
v0x55865995cf20_0 .var "clk", 0 0;
v0x55865995cff0_0 .var "rst", 0 0;
S_0x558659948f80 .scope module, "IF_Unit" "IF" 2 9, 3 1 0, S_0x558659948df0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "PC_SRC";
    .port_info 3 /INPUT 32 "PC_IN";
    .port_info 4 /OUTPUT 32 "PC_OUT";
v0x558659910ba0_0 .net "PC_IN", 31 0, v0x55865995cc70_0;  1 drivers
v0x55865995c850_0 .var "PC_OUT", 31 0;
v0x55865995c930_0 .net "PC_SRC", 0 0, v0x55865995ce20_0;  1 drivers
v0x55865995ca00_0 .net "clk", 0 0, v0x55865995cf20_0;  1 drivers
v0x55865995cac0_0 .net "rst", 0 0, v0x55865995cff0_0;  1 drivers
E_0x55865990f6f0/0 .event negedge, v0x55865995cac0_0;
E_0x55865990f6f0/1 .event posedge, v0x55865995ca00_0;
E_0x55865990f6f0 .event/or E_0x55865990f6f0/0, E_0x55865990f6f0/1;
    .scope S_0x558659948f80;
T_0 ;
    %wait E_0x55865990f6f0;
    %load/vec4 v0x55865995cac0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55865995c850_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55865995c930_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.2, 8;
    %load/vec4 v0x558659910ba0_0;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %load/vec4 v0x55865995c850_0;
    %addi 4, 0, 32;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %assign/vec4 v0x55865995c850_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x558659948df0;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55865995cf20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55865995cff0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55865995ce20_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55865995cf20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55865995cff0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55865995cf20_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55865995cf20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55865995cff0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55865995cf20_0, 0, 1;
    %end;
